{
  "name": "app",
  "version": "1.0.0",
  "description": "test",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "pretest": "npm run clean && npm run build",
    "test": "NODE_ENV=test lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "migrate": "NODE_ENV=production node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.2.7",
    "@loopback/context": "^1.15.0",
    "@loopback/core": "^1.7.0",
    "@loopback/openapi-v3": "^1.3.11",
    "@loopback/repository": "^1.5.5",
    "@loopback/rest": "^1.11.2",
    "@loopback/rest-explorer": "^1.1.22",
    "@loopback/service-proxy": "^1.1.10",
    "@types/dotenv": "^6.1.1",
    "dotenv": "^8.0.0",
    "loopback-connector-mysql": "^5.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.5.4",
    "@loopback/testlab": "^1.2.9",
    "@loopback/tslint-config": "^2.0.4",
    "@types/node": "^10.11.2",
    "typescript": "^3.4.3"
  }
}
